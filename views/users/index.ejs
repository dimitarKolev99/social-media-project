<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Users</title>
</head>
<body>
    <h2>Users Table</h2>
    <table class="table">
        <thead>
            <tr>
                <th>Username</th>
                <th>Email</th>
            </tr>
        </thead>
        
        <tbody>
            <% users.forEach(user => { %>
            <tr>
                <td><%= user.username %></td>
                <td><%= user.email %></td>
            </tr>
            <% }); %>
        </tbody>

    </table>
    
</body>
</html>